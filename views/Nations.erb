<% ctr=1 %>
<div style="background:url('images/statue.jpg'); background-size:100%; height:500px; color:black"; >
<BR>
 <CENTER><TABLE BORDER='1' class="main">
 <TD colspan="10"  BGCOLOR="#24883F" style="font-size: 25px; color:black;"><CENTER>Nations</CENTER></TD><TR>
 <% @nations.each do |nation| %>


 <TD class="data" WIDTH="200">
    
    <%= nation.name %></B>
    <TD WIDTH="10" class="flag" BGCOLOR="#060E6D" PADDING="0"><CENTER><IMG SRC="flags/<%= nation.name %>.png" </TD>
    
    <% if ctr==5 %>
      </TD><TR>
      <% ctr=1 %>
    <% else %>
      <% ctr +=1 %>
      </TD>
    <% end %>

 <% end %>
 </TABLE>
 </div>